{
    "name": "xnf4o/litres",
    "description": "Laravel Litres package",
    "license": "MIT",
    "authors": [
        {
            "name": "xnf4o",
            "email": "xnf4o@inbox.ru"
        }
    ],
    "homepage": "https://github.com/xnf4o/litres",
    "keywords": ["Laravel", "litres"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "xnf4o\\litres\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xnf4o\\litres\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xnf4o\\litres\\litresServiceProvider"
            ],
            "aliases": {
                "litres": "xnf4o\\litres\\Facades\\litres"
            }
        }
    }
}
